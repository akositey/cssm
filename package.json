{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix && npm run ssr:build",
    "fix:eslint": "eslint --ext .vue resources/js/ --fix",
    "fix:prettier": "prettier --write --loglevel warn 'resources/js/**/*.vue'",
    "fix-code-style": "npm run fix:prettier && npm run fix:eslint",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production && npm run build",
    "heroku-postbuild": "npm run prod"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^8.2.0"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@inertiajs/inertia": "^0.11.0",
    "@inertiajs/inertia-vue3": "^0.6.0",
    "@nesk/puphpeteer": "^2.0.0",
    "autoprefixer": "10.4.5",
    "axios": "^0.27.2",
    "chart.js": "^3.8.0",
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.41",
    "lodash": "^4.17.21",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.14",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwind": "^2.2.12",
    "process": "^0.11.10",
    "puppeteer": "^14.1.0",
    "resolve-url-loader": "^3.1.4",
    "signature_pad": "^3.0.0-beta.4",
    "tailwindcss": "^2.2.19",
    "uuid": "^8.3.2",
    "vue": "^3.2.27",
    "vue-chartjs": "^4.1.0",
    "vue-datepicker-next": "^1.0.2",
    "vue-eslint-parser": "^9.0.2",
    "vue-loader": "^16.2.0",
    "ziggy-js": "^1.4.6"
  }
}
